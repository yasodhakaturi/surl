@model Analytics.Models.UserViewModel
@{
    ViewBag.Title = "Analytics";
}
<!DOCTYPE html>
<html ng-app="bitraz">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1"> <!-- Page title set in pageTitle directive -->
    <title page-title>biTRAZ :: trace your link</title> <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
     <link rel="shortcut icon" type="image/ico" href="favicon.ico" />
    <link rel="stylesheet" href="../dist/analytics/styles/vendor.css">
    <link rel="stylesheet" href="../dist/analytics/styles/style.css">


    <script type="text/javascript">
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-93487896-1', 'auto');
        ga('send', 'pageview');

        (function () {
            window.appConfig = {
                env: "@Model.env",
                apiUrl: '@Model.appUrl',
                appUrl:{admin: "@Model.apiUrl.admin",analytics:"@Model.apiUrl.analytics",landing:"@Model.apiUrl.landing"},
                userInfo: { user_id: @Model.userInfo.user_id, UserName: "@Model.userInfo.user_name", isAdmin: @Model.isAdmin, isClient: @Model.isClient, isReadOnly: true }
            }
        })();
    </script>
</head>
    @Html.Partial("~/Views/Analytics/AnalyticsUI.cshtml")
    @*@Html.Partial("~/Views/Home/IndexUI.cshtml")*@
    
</html>
