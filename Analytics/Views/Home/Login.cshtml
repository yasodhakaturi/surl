@model Analytics.Models.UserViewModel
@{
    ViewBag.Title = "Login";
}
<script type="text/javascript">
    (function () {
        window.appConfig = {
            env: "@Model.env",
            apiUrl: '@Model.appUrl',
            appUrl:{admin: "@Model.apiUrl.admin",analytics:"@Model.apiUrl.analytics",landing:"@Model.apiUrl.landing"},
            userInfo: { Id: @Model.userInfo.user_id, UserName: @Model.userInfo.user_name, isAdmin: @Model.isAdmin, isClient: @Model.isClient, isReadOnly: true }
        }
    })();</script>

@Html.Partial("~/Views/Home/LoginUI.cshtml")
